@model IEnumerable<AddingModels.Models.Product>
   
@{
    ViewBag.Title = "Product";
}
<h2>Product</h2>
<table border="1" class="table table-bordered table-responsive">
    <thead>
        <tr class="btn-primary">
            <th>Product ID</th>
            <th>Product Name</th>
            <th>Order ID</th>
            <th>Quantity</th>
        </tr>
    </thead>   
    <tbody>
        @foreach(var item in Model)
            {         
        <tr>
            <td>@Model.ProductID</td>
            <td>@Model.ProductID</td>
        </tr>
            }     
    </tbody>
</table>

